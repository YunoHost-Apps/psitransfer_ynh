{
    "name": "PSITransfer",
    "id": "psitransfer",
    "packaging_format": 1,
    "description": {
        "en": "Transfer files or images without problem",
	      "fr": "TransfÃ©rer des fichiers sans soucis"
    },
    "version": "1.12.0~ynh1",
    "url": "https://psi.cx/tags/PsiTransfer/",
    "upstream": {
        "license": "BSD-2-Clause",
        "website": "https://psi.cx/tags/PsiTransfer",
        "admindoc": "https://github.com/psi-4ward/psitransfer/tree/master/docs",
        "code": "https://github.com/psi-4ward/psitransfer"
    },
    "license": "BSD-2-Clause",
    "maintainer": {
        "name": "frju365",
        "email": "win10@tutanota.com",
        "url": "https://github.com/frju365/psitransfer_ynh"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/psitransfer",
                "default": "/psitransfer"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
